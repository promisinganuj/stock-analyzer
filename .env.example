# Copy to .env and fill values
ALPHA_VANTAGE_KEY=your_alpha_vantage_key  # required only if PRICE_PROVIDER=alphavantage
FINNHUB_KEY=your_finnhub_key
FMP_KEY=your_financialmodelingprep_key

# Price data source routing (required; defaults to stooq).
# - stooq: free, no-key EOD prices via https://stooq.com
# - alphavantage: requires ALPHA_VANTAGE_KEY
PRICE_PROVIDER=stooq

AZURE_OPENAI_KEY=your_azure_openai_key
AZURE_OPENAI_ENDPOINT=https://your-openai-resource.openai.azure.com/
